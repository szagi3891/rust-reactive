{
    "compilerOptions": {
        "strict": true,
        "target": "ES6",
        "lib": ["dom", "es6", "esnext"],
        "downlevelIteration": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noUncheckedIndexedAccess": true,
        "esModuleInterop": true,
        "baseUrl": ".",
        "noEmit": true
    }
}
